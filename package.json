{
  "name": "@pakerzhang/rename-cli",
  "version": "1.1.4",
  "description": "Rename files tool",
  "type": "module",
  "exports": "./src/index.mjs",
  "engines": {
    "node": ">=16"
  },
  "bin": {
    "rename-image": "./src/bin/cli.mjs"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "node --trace-warnings ./src/bin/cli.mjs",
    "build": "pkg .  --no-bytecode"
  },
  "author": "Paker Zhang",
  "repository": {
    "type": "git",
    "url": "git@github.com:pakerchang/Rename-tool.git"
  },
  "homepage": "https://github.com/pakerchang/Rename-tool",
  "bug": "https://github.com/pakerchang/Rename-tool/issues",
  "keywords": [
    "rename image tool",
    "tool",
    "node"
  ],
  "files": [
    "src"
  ],
  "pkg": {
    "scripts": [
      "./src/**/*.{mjs,js,cjs}"
    ],
    "assets": [
      "./src/**/*.{mjs,js,cjs}",
      "./node_modules"
    ],
    "outputPath": "dist"
  },
  "license": "MIT",
  "dependencies": {
    "chalk": "^5.2.0",
    "inquirer": "^9.1.4",
    "nanospinner": "^1.1.0"
  }
}
